{
    "name": "artesaos/laravel-linkedin",
    "type": "library",
    "description": "Linkedin API integration for Laravel and Lumen 5",
    "keywords": ["LinkedIn","Laravel Framework","Lumen","API","Client","SDK","Integration","REST"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mauri de Souza Nunes",
            "email": "mauri870@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0",
        "happyr/linkedin-api-client": "~1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "php-http/message": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.4@dev"
    },
    "autoload": {
        "psr-4": {
            "Artesaos\\LinkedIn\\": "src/Artesaos/LinkedIn/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artesaos\\LinkedIn\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artesaos\\LinkedIn\\LinkedinServiceProvider"
            ],
            "aliases": {
                "LinkedIn": "Artesaos\\LinkedIn\\Facades\\LinkedIn"
            }
        }
    }
}
